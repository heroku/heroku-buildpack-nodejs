@bot.message_handler(func=lambda message: message.text == "الاوامر")
تعريف lock_images(رسالة):

    user_id = message.from_user.id
    chat_id = message.chat.id
    العضو = bot.get_chat_member(chat_id, user_id)

    إذا كان member.status في ["المسؤول"، "المنشئ"]:
        
        إذا لم يكن معرف الدردشة موجودًا في مجموعات_مقفلة:
            lock_groups.append(chat_id)
        bot.reply_to(message, """⌁︙اهلا عزيزي الادمن 🧜🏻 .
⌁︙ اوامر حماية ارسل ↫`M1`
⌁︙اوامر الالعاب ارسل ↫`M2`
⌁︙اوامر التسلية ارسل ↫`M3`
⌁︙اوامر اخرى ارسل ↫`M4`
 ⌁︙اضغط على الأمر لـ النسخ 👾""",parse_mode="markdown")
    آخر:
        bot.reply_to(message, "⌁︙انت مو ادمن ياعضو 💃🏻 !")
                   
