@bot.message_handler(func=lambda message: message.text == 'المالك')
تعريف get_group_owner (الرسالة):
    إذا message.chat.type == 'supergroup' أو message.chat.type == 'group':
        chat_id = message.chat.id
        chat_admins = bot.get_chat_administrators(chat_id)

        للمشرف في chat_admins:
            إذا admin.status == "منشئ":
                Owner_id = admin.user.id
                Owner_username = admin.user.username
                اسم المالك = admin.user.first_name

                إذا كان hasattr(admin.user, 'photo'):
                    Owner_photo_id = admin.user.photo.big_file_id
                    bot.send_photo(message.chat.id,owner_photo_id)

                الرد = f'''- 𝐆𝐑𝐎𝗨𝐏 𝐎𝐖𝐍𝐄𝐑 ✅ |

• حقوق النشر :- @{owner_username}
• 𝐍𝐀𝐌𝐄 :- {owner_name}
• 𝐈𝐃 :- {owner_id}''
                bot.reply_to(رسالة، رد)
                يعود
        bot.reply_to(message, 'لا يمكن العثور على معلومات المالك في هذه المجموعة.')

    آخر:
        bot.reply_to(message, 'هذا الأمر يمكن استخدامه في المجموعات فقط.')

@bot.message_handler(func=lambda message: message.text.startswith('حظر') وmessage.reply_to_message ليس بلا)
حظر نهائي (رسالة):
    إذا str(message.from_user.id) != admin_id:
        bot.send_message(message.chat.id, "أنت مو مشرف")
    آخر:
        سيد = message.chat.id
        uid = message.reply_to_message.from_user.id
        bot.kick_chat_member(cid, uid)
        bot.send_message(cid, "تم حظر العضو الفعال")

@bot.message_handler(func=lambda message: message.text.startswith('الغاء الأمريكي') و message.reply_to_message ليس لا شيء)
def unban(الرسالة):
    إذا str(message.from_user.id) != admin_id:
        bot.send_message(message.chat.id, "أنت مو مشرف")
    آخر:
        سيد = message.chat.id
        uid = message.reply_to_message.from_user.id
        bot.unban_chat_member(cid, uid)
        bot.send_message(cid, "تم إلغاء الحظر الفعال")                           
كتم_المستخدمين = {}

@bot.message_handler(func=lambda message: message.text == 'تقييد')
تعريف Handle_mute(الرسالة):
    chat_id = message.chat.id
    user_id = message.reply_to_message.from_user.id
    bot.restrict_chat_member(chat_id, user_id, can_send_messages=False)
    bot.reply_to(message.reply_to_message, "*- تم تجميعك في المجموعة 📛 .*",parse_mode="Markdown")

@bot.message_handler(func=lambda message: message.text == 'الغاء السكنيد')
تعريف Handle_unmute(الرسالة):
    chat_id = message.chat.id
    user_id = message.reply_to_message.from_user.id
    bot.restrict_chat_member(chat_id, user_id, can_send_messages=True)
    bot.reply_to(message.reply_to_message, "*- تم إلغاء تنشيطك بفعالية ✅ .*",parse_mode="Markdown")

@bot.message_handler(func=lambda message: message.reply_to_message و message.reply_to_message.from_user.id == bot.get_me().id)
حدد Handle_user_messages(رسالة):
    chat_id = message.chat.id
    user_id = message.from_user.id
    إذا كان user_id في muted_users وmuted_users[user_id] == chat_id:
        يحاول:
            bot.delete_message(chat_id, message.message_id)
        باستثناء telebot.apihelper.ApiTelegramException كـ e:
            إذا "لم يتم العثور على رسالة الحذف" في str(e):
                bot.send_message(chat_id, "الرسالة التي طلبت حذفها غير موجودة")
            آخر:
                رفع ه

@bot.message_handler(func=lambda message: message.reply_to_message و message.reply_to_message.from_user.id == bot.get_me().id)
تعريف Handle_mute_unmute_confirmation(الرسالة):
    user_id = message.reply_to_message.from_user.id
    chat_id = message.chat.id
    if message.text == 'تقييد':
        muted_users[user_id] = chat_id
        bot.reply_to(message.reply_to_message, "*- تم صنع شخص ✅ .*",parse_mode="Markdown")
    elif message.text == ' إلغاء شيء':
        إذا كان user_id في muted_users و muted_users[user_id] == chat_id:
            ديل كتم_المستخدمين[user_id]
            bot.reply_to(message.reply_to_message, "*تم إلغاء التثبيت*",parse_mode="Markdown")

@bot.message_handler(func=lambda message: message.text and message.reply_to_message and message.text.startswith("تقييد وقتي"))
تعريف تقييد_المستخدم (الرسالة):
    إذا لم يكن message.from_user.id موجودًا في [admin.user.id للمسؤول في bot.get_chat_administrators(message.chat.id)]:
        bot.reply_to(message, "᥀ انت مو مشرف ᥀")
        يعود
    user_id = message.reply_to_message.from_user.id
    bot.restrict_chat_member(message.chat.id, user_id, until_date=int(time.time())+600)
    bot.reply_to(message.reply_to_message, "᥀ تم تعجبك بطريقة مذهلة وقتي ᥀")
    الوقت.النوم(600)
    bot.restrict_chat_member(message.chat.id, user_id, can_send_messages=صحيح, can_send_media_messages=صحيح, can_send_other_messages=صحيح, can_add_web_page_previews=True)
    bot.reply_to(message.reply_to_message, "᥀ تم انتهاء المدة لسببك هسه تدر تحجي براحتك حبيبي ᥀")

مجموعات_مقفلة = []

@bot.message_handler(func=lambda message: message.text == "قفل الصور")
تعريف lock_images(رسالة):
    user_id = message.from_user.id
    chat_id = message.chat.id
    العضو = bot.get_chat_member(chat_id, user_id)

    إذا كان member.status في ["المسؤول"، "المنشئ"]:
        إذا لم يكن معرف الدردشة موجودًا في مجموعات_مقفلة:
            lock_groups.append(chat_id)
        
        bot.reply_to(message, "*- تم قفل الصور الفعالة ✅*", parse_mode="markdown")
    آخر:
        bot.reply_to(message, "⌁︙انت مو ادمن ياعضو 💃🏻 !")

@bot.message_handler(func=lambda message: message.text == "فتح الصور")
تعريف unlock_images(الرسالة):
    user_id = message.from_user.id
    chat_id = message.chat.id
    العضو = bot.get_chat_member(chat_id, user_id)

    إذا كان member.status في ["المسؤول"، "المنشئ"]:
        إذا كان chat_id في مجموعات_مقفلة:
            lock_groups.remove(chat_id)
        
        bot.reply_to(message, "*- تم فتح الصور الفعالة *✅", parse_mode="markdown")
    آخر:
        bot.reply_to(message, "⌁︙انت مو ادمن ياعضو 💃🏻 !")

@bot.message_handler(content_types=['صورة'])
تعريف Handle_photos(الرسالة):
    user_id = message.from_user.id
    chat_id = message.chat.id
    العضو = bot.get_chat_member(chat_id, user_id)

    إذا كان معرف الدردشة في المجموعات المقفلة وحالة العضو ليس في ['المسؤول'، 'المنشئ']:
        bot.delete_message(chat_id, message.message_id)

@bot.message_handler(content_types=["photo"])
حدد حذف_الصور(الرسالة):
    chat_id = message.chat.id
    
    إذا كان chat_id في مجموعات_مقفلة:
        
        bot.delete_message(chat_id, message.message_id)

@bot.message_handler(func=lambda message: message.text.startswith('انطق'))
تعريف text_to_speech (الرسالة):

    النص = message.text.split(' ', 1)[1]


    تحويل النص إلى كلام = gTTS(text=text, lang='ar')
    تحويل النص إلى كلام.حفظ('audio.mp3')

    الصوت = مفتوح ('audio.mp3'، 'rb')
    bot.send_audio(chat_id=message.chat.id, audio=audio, renderer= '- @MMMMFi', title='- تم تحويل 🧜🏻 .')

    os.remove('audio.mp3')

@bot.message_handler(func=lambda message: message.text.lower() == 'نرد')
def roll_dice(الرسالة):
    النرد = عشوائي.راندنت (1، 6)
    إذا النرد == 6:
        bot.reply_to(message, """مبررررررررروووووك 🥹🎉 !
اخيرا فزت على 6 👾 !
يامحظوظ يا محظوظ 😉 .""")

    آخر:
        bot.reply_to(message, """للاسف حظك مو حلو وما حصلت 6 💔 .
حظ اوفر اذا تريد اكتب ( `نرد` ) """,parse_mode="markdown")

