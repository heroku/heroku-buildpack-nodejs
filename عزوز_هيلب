@bot.message_handler(func=lambda message: message.text == 'الرابط' و message.reply_to_message لا شيء)
بالتأكيد send_group_link (الرسالة):
    chat_id = message.chat.id
    group_link = bot.export_chat_invite_link(chat_id)
    bot.reply_to(message, f"*- 𝐆𝐑𝐎𝗨𝐏 𝐋𝐈𝐍𝐊 :-* {group_link}",parse_mode="Markdown")

@bot.message_handler(func=lambda message: message.text == "الاسرع")
تعريف start_game (الرسالة):
    words = ['محمد','فيل','دجاجه','ثلاجه','مدرسه','عطارد','العراق','مصر','السعودية','حمودي','الامير','اسد' ,'كرسي','كارتون','سجق','خوخ','برتقال','موز','عسل','ساعه','كهرباء','سياره','شمس','قمر',' نجوم','ياسمين','الجزائر','طياره','سفينه','شاحنه','شركه','ضوء','ظلام','فواكه','سمسم','شاورما','باذنجان' ,'طماطم']
    Random_word = اختيار عشوائي (كلمات)
    chat_id = message.chat.id
    bot.send_message(chat_id, "- أسرع شخص يكتب:" + كلمة عشوائية)
    bot.register_next_step_handler(الرسالة، كلمة التحقق، كلمة عشوائية)

تعريف check_word (رسالة، كلمة عشوائية):
    user_word = message.text
    chat_id = message.chat.id

    إذا كان user_word == كلمة عشوائية:
        bot.reply_to(message, "أحسنت ، انت الفائز 🥳")
    آخر:
        bot.reply_to(message, "للأسف لم تكن أنت الاسرع، حظ اوفر في المره القادمة 😇")

lock_stickers = خطأ

@bot.message_handler(content_types=["ملصق"])
Deflete_stickers(الرسالة):
    إذا كانت ملصقات_مقفلة:
        المستخدم = bot.get_chat_member(message.chat.id, message.from_user.id)
        إذا لم يكن user.status موجودًا في ['administrator'، 'creator']:
            bot.delete_message(message.chat.id, message.message_id)

@bot.message_handler(func=lambda message: message.text == 'قفل الملصقات')
تعريف lock_stickers (الرسالة):
    المستخدم = bot.get_chat_member(message.chat.id, message.from_user.id)
    إذا كان user.status في ["المسؤول"، "المنشئ"]:
        ملصقات مقفلة عالمية
        lock_stickers = صحيح
        bot.reply_to(message, '*- تم إغلاق الملصقات الفعالة ✅ .*',parse_mode="markdown")

@bot.message_handler(func=lambda message: message.text == 'فتح الملصقات')
إلغاء إلغاء القفل_ملصقات(رسالة):
    المستخدم = bot.get_chat_member(message.chat.id, message.from_user.id)
    إذا كان user.status في ["المسؤول"، "المنشئ"]:
        ملصقات مقفلة عالمية
        ملصقات مقفلة = خطأ
        bot.reply_to(message, '*- تم فتح الملصق بنجاح ✅ .*',parse_mode="markdown")

@bot.message_handler(func=lambda message: message.text in ['قفل ​​الملصقات', 'فتح الملصقات'])
تجاهل تجاهل_الأوامر (الرسالة):
    المستخدم = bot.get_chat_member(message.chat.id, message.from_user.id)
    إذا لم يكن user.status موجودًا في ['administrator'، 'creator']:
        bot.delete_message(message.chat.id, message.message_id)
                               
تحذيرات = {}
تقييد_المستخدمين = []
@bot.message_handler(func=lambda message: message.chat.type == 'group' or message.chat.type == 'supergroup')
تعريف Handle_group_message(رسالة):
    إذا message.reply_to_message وis_admin(message.from_user.id, message.chat.id):
        user_id = message.reply_to_message.from_user.id
        إذا كان user_id في المقيدين_المستخدمين:
            bot.reply_to(message, "*- تم تحذير هذا الشخص 3 مرات أولاً وتم تقييده، لـ تقييد الشخص مرة أخرى ارسل الأمر* `اتقييد` *بالرد علية 📛.*",parse_mode="Markdown")
        آخر:
            إذا لم يكن user_id موجودًا في التحذيرات:
                تحذيرات [user_id] = 1
            آخر:
                تحذيرات[user_id] += 1
            إذا كانت التحذيرات[user_id] == 3:
                bot.restrict_chat_member(message.chat.id, user_id, until_date=0)
                المقيدة_users.append(user_id)
                bot.reply_to(message, f"*- تم تغيير المستخدم* [{message.reply_to_message.from_user.first_name}](tg://user?id={user_id}) *بنجاح بسبب مخالفة ثلاث مرات!* ✅"،parse_mode ="تخفيض السعر")
            آخر:
                تحذير_رسالة = "تحذير!"
                إذا كانت التحذيرات [user_id] == 2:
                    تحذير_message += f" لديك *تحذيرين* من اصل *ثلاث، كنذراً ⚠️.*\n\n[{message.reply_to_message.from_user.first_name}](tg://user?id={user_id})"
                آخر:
                    تحذير_message += f" تحذير *واحد* من اصل *ثلاث كناً ⚠️.*\n\n[{message.reply_to_message.from_user.first_name}](tg://user?id={user_id})"
                bot.reply_to (رسالة، تحذير_رسالة، parse_mode = "Markdown")
تعريف is_admin(user_id, chat_id):
    المشرفين = bot.get_chat_administrators(chat_id)
    admin_ids = [admin.user.id للمسؤول في المسؤولين]
    إرجاع user_id في admin_ids

